package introducaoJava;
import java.util.Scanner;
public class ex001 {
	public static void main(String[] args) {
		int idadeAnos, idadeMeses, idadeDias;
		Scanner leia = new Scanner(System.in);
	
		System.out.println("\nInforme sua idade em anos: ");
		idadeAnos = leia.nextInt();
		System.out.println("Informe sua idade em meses: \n");
		idadeMeses = leia.nextInt();
		System.out.println("Informe sua idade em dias: \n");
		idadeDias = leia.nextInt();
		
		
		
	}

}
